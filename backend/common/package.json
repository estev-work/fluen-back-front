{
  "name": "common",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/typeorm": "^9.0.1",
    "mysql": "^2.18.1",
    "typeorm": "^0.3.11",
    "rxjs": "^7.8.0",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "@nestjs/microservices": "^9.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "mysql": "^2.18.1",
    "typeorm": "^0.3.11",
    "class-validator": "^0.14.0",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@nestjs/common": "^9.2.1",
    "@nestjs/core": "^9.2.1",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/testing": "9.2.1",
    "@types/express": "4.17.15",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/supertest": "2.0.12",
    "jest": "^29.3.1",
    "prettier": "2.8.2",
    "supertest": "6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0",
    "tsconfig-paths": "^4.1.2",
    "tslint": "^6.1.3",
    "typescript": "^4.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
